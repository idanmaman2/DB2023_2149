type Movie {
    id: ID!
    name: String!
    description: String!
    rating: String!
    poster_images: Object!
    trailer: String!
    reales_date: String!
    duration: String!
}

type Query {
    findAllMovies: [Movie!]
    findMoviesByRating(ratings:[String!]!): [Movie]!
    findMovieById(id:Int!):Movie
}

scalar Object